
n = int(raw_input())

s1 = list(raw_input())
s2 = list(raw_input())

maior = menor = 0

if "?" not in s1 and "?" not in s2:
    for i in xrange(n):
        if s1[i] >= s2[i]:
            maior = 1
        else:
            menor = 1

        if maior == 1 and menor == 1:
            print 1
            exit(0)
            
    print 0
    exit(0)

maior = menor = 0
antes = depois = 0
poss = 1

for i in xrange(n):

    if s1[i] != "?":
        if s2[i] != "?":
            if int(s1[i]) > int(s2[i]):
                maior += 1
            else:
                menor += 1

        else:
            antes *= int(s1[i])
            depois *= 10 - int(s1[i])
            poss *= 10
    else:
        if s2[i] != "?":
            antes *= int(s2[i])
            depois *= 10 - int(s2[i])
            poss *= 10
        else:
            antes *= 40
            depois *= 40
            poss *= 100

if maior != 0 and menor != 0:
    print poss
    exit(0xs)
            
print poss - antes - depois
