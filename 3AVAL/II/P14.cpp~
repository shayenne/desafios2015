#include<iostream>
#include<vector>

using namespace std;

int main() {
  int t, n, m;
  vector<int> A (101), B (101);
  vector<list<int> > graph (101); 
  
  cin >> t;

  for (int i = 0; i < t; i++) {
    cin >> n;

    for (int j = 1; j <= n; j++) {
      cin >> A[j]; 
    }

    cin >> m;

    for (int j = 1; j <= m; j++) {
      cin >> B[j]; 
    }

    for (int j = 1; j <= n; j++) {
      for (int k = 1; k <= m; k++) {
	if (A[j] % B[k] == 0) {
	  graph[A[j]].insert(B[k]);
	  graph[B[k]].insert(A[j]);
	}
      }
    }
    
  }
  
  return 0;
}
