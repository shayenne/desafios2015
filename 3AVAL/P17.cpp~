#include<iostream>
#include<stdio.h>
#include <functional>
#include <queue>
#include <vector>

using namespace std;

int main() {
  int n;
  int x, y;
  pair<int, int> p;
  vector<pair<float, pair<int, int> > > q;
    
  cin >> n;

  for (int i = 0; i < n; i++) {
    cin >> x >> y;
    p = make_pair (x, y);
    q[i] = make_pair (0, p);
  }

  for (int i = 0; i < n/2; i++) {
    cout << q[i].first << " " << q[i].second << endl;
  }
  
  return 0;
}
