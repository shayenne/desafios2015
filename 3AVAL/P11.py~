n = int(raw_input())

city = []

for i in xrange(n):

    city.append(map(int, raw_input().split()))


R, r = map(int, raw_input().split())

if   R < r:
    print 0
elif R == r:
    print 1
else:
    total = 1
    num = 0
    for i in xrange(n):
        for j in xrange(i+1, n):
            dist = math.sqrt((city[i][0]-city[j][0])*(city[i][0]-city[j][0]) +
                             (city[i][1]-city[j][1])*(city[i][1]-city[j][1]))
            if dist + 2*r <= 2*R:
                num += 1

        if total < num:
            total = num

    print total
